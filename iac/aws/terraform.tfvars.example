# Example Terraform variables for deploying Hero Image Processor Lambda
# Copy to terraform.tfvars and fill in actual values

aws_region = "us-east-1"

environment = "dev"

cloudflare_r2_account_id = "your-r2-account-id-here"

cloudflare_r2_access_key_id = "your-r2-access-key-here"

cloudflare_r2_secret_access_key = "your-r2-secret-key-here"

cloudflare_r2_bucket_name = "hubletix"

# This will be generated by the build/deployment process
# Do not manually edit unless updating Lambda code outside of CI/CD
lambda_source_code_hash = "placeholder"
